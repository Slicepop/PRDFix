{
  "manifest_version": 3,
  "name": "PRD Upgrade",
  "version": "1.0",
  "description": "Extension used for the PRD",
  "permissions": ["activeTab"],

  "content_scripts": [
    {
      "matches": ["https://idm.int.wmed.edu/*"],
      "js": ["content.js"],
      "run at": "document_idle"
    }
  ]
}
